{
  "compilerOptions": {
    "target": "es3",
    "module": "commonjs",
    "lib": ["es2015", "es2017", "dom"],
    "outDir": "./dist",
    "rootDir": "./src",
    "removeComments": false,
    "downlevelIteration": true,
    "strict": true,
    "alwaysStrict": true,
    "noImplicitReturns": true,
    "moduleResolution": "node"
  }
}